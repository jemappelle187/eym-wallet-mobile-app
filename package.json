{
  "name": "sendnreceive-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:clean": "lsof -ti :8081 | xargs -r kill -9; lsof -ti :8082 | xargs -r kill -9; watchman watch-del-all || true; rm -rf ~/.expo/metrocache ~/.cache/expo; npx expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "mobile:dev": "EXPO_PUBLIC_API_BASE=$EXPO_PUBLIC_API_BASE EXPO_PUBLIC_WEBHOOK_SECRET=$EXPO_PUBLIC_WEBHOOK_SECRET expo start --clear",
    "metro:bg": "nohup npx expo start --localhost --dev-client > /tmp/metro.log 2>&1 & echo $! > /tmp/metro.pid",
    "metro:tail": "tail -f /tmp/metro.log",
    "metro:stop": "kill $(cat /tmp/metro.pid) 2>/dev/null || true && lsof -ti :8081 | xargs -r kill",
    "ios:native": "npx expo run:ios"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@invertase/react-native-apple-authentication": "^2.4.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@react-navigation/stack": "^7.4.2",
    "@stripe/stripe-react-native": "0.50.3",
    "expo": "54.0.8",
    "expo-blur": "~15.0.7",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "^18.0.8",
    "expo-dev-client": "~6.0.12",
    "expo-device": "~8.0.8",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-local-authentication": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "express": "^5.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-paypal-wrapper": "^1.0.0",
    "react-native-plaid-link-sdk": "^8.0.1",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^19.1.0",
    "@types/react": "^19.1.13",
    "typescript": "~5.9.2"
  },
  "private": true
}
