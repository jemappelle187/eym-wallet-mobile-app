{
  "expo": {
    "name": "sendnreceive-app",
    "slug": "sendnreceive-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "jsEngine": "hermes",
      "bundleIdentifier": "com.jemappelle187.sendnreceiveapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSFaceIDUsageDescription": "SendNReceive uses FaceID to securely authenticate you and protect your wallet."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "jsEngine": "hermes",
      "package": "com.jemappelle187.sendnreceiveapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "privacy": "public",
    "privacyPolicyUrl": "https://example.com/privacy-policy",
    "extra": {
      "eas": {
        "projectId": "9afa6546-591d-49a7-8e28-4cb54938dd65"
      }
    }
  }
}
